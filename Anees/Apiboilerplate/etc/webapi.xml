<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="GET" url="/V1/Apiboilerplate/getproductname/:sku">
		<service class="Anees\Apiboilerplate\Api\GetProductNameManagementInterface" method="getGetProductName"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>

    <route method="GET" url="/V1/Apiboilerplate/getproductprice/:sku">
        <service class="Anees\Apiboilerplate\Api\GetProductPriceInterface" method="getGetProductPrice"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

</routes>
